{
  "name": "rosie-frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.1.1"
  },
  "dependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier-standard": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "format": "prettier-standard 'src/**/*.js'"
  },
  "lint-staged": {
    "{,!(node_modules)/}**/*.{js,jsx}": [
      "prettier-standard",
      "git"
    ]
